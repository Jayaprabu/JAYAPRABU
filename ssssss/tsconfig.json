{
    "compilerOptions":{
        "jsx": "react",
        "module": "commonjs",
        "noImplicitAny": true,
        "outDir": "./build/",
        "preserveConstEnums": true,
        "removeComments": true,
        "target": "es6" ,
        "lib":["es2015","dom"],
        "sourceMap": true,
        "experimentalDecorators": true,
        "moduleResolution": "node",
        "typeRoots": [
            "Models", "typings", "node_modules/@types"
        ]
    },
    "exclude":["node_modules"]
}
